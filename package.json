  "description": "A cms system based on koa2 & koa-router",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon app.ts",
    "prd": "cross-env NODE_ENV=production pm2 start pm2.conf.json",
    "start": "concurrently \"npm run admin\"  \"npm run dev\"",
    "admin": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.conf.js",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.prod.conf.js",
    "report": "npm run build --report"
  },
